#root {
  max-width: 1280px;
  margin: 0 auto;
  padding: 2rem;
  text-align: center;
  
}

.container {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 70rem;

  background-color: #F5F5FA;
  border: 1px solid black;
  
  height: 100vh; 
  color: black; 
  margin: 0;
  padding: 0;
  display: flex;
  flex-direction: column;

}